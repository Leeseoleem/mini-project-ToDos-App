    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-build-properties"],
    "extra": {
      "eas": {
        "projectId": "e6062830-9afc-49c2-82a4-911ed64209f5"
      }
